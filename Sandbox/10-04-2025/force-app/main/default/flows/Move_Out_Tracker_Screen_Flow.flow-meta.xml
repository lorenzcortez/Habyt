    <decisions>
        <name>Contract_End_Date_1_Contract_Start_Date</name>
        <label>(Contract End Date - 1) &gt; Contract Start Date</label>
        <locationX>578</locationX>
        <locationY>242</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Contract_End_Date_1_Contract_Start_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FormulaContractEndDate</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>Contract.StartDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Member_Services_Queue</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Priority_Check</name>
        <label>Priority Check</label>
        <locationX>798</locationX>
        <locationY>998</locationY>
        <defaultConnector>
            <targetReference>Assign_Task_to_List</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Contract_End_Date_1_30_days</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FormulaContractEndDatePriority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>30.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Subject_Loop</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Move-out instructions sent</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Priority_to_High</targetReference>
            </connector>
            <label>(Contract End Date - 1) &lt; 30 days</label>
        </rules>
        <rules>
            <name>Contract_End_Date_1_TODAY</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FormulaContractEndDatePriority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Subject_Loop</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Keys returned</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <isGoTo>true</isGoTo>
                <targetReference>Set_Priority_to_High</targetReference>
            </connector>
            <label>(Contract End Date - 1) &lt; TODAY</label>
        </rules>
        <rules>
            <name>Contract_End_Date_1_1_week</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FormulaContractEndDatePriority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>7.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Subject_Loop</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Bank details collected</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <isGoTo>true</isGoTo>
                <targetReference>Set_Priority_to_High</targetReference>
            </connector>
            <label>(Contract End Date - 1) &lt; 1 week</label>
        </rules>
    </decisions>
