            <name>EmergencyInfo</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: rgb(42, 42, 42); font-family: &amp;quot;Open Sans&amp;quot;, sans-serif; font-size: 20px;&quot;&gt;Please review emergency resources below. Only use this button if this is an actual emergency. Please note: This will post in the MSS slack channel.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;a href=&quot;https://www.google.com/url?q=https://docs.google.com/document/d/1kJZndTfw_nvwkDwcg2EGaelPvmMsUE9USwb9z_Jvydc/edit%23&amp;amp;sa=D&amp;amp;ust=1610125283000000&amp;amp;usg=AOvVaw3Tp0NbdpAZ-Mv6LyXqMHg5&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot; style=&quot;background-color: rgb(255, 255, 255); color: rgb(26, 115, 232); font-family: Roboto, RobotoDraft, Helvetica, Arial, sans-serif; font-size: 20px;&quot;&gt;https://docs.google.com/document/d/1kJZndTfw_nvwkDwcg2EGaelPvmMsUE9USwb9z_Jvydc/edit#&lt;/a&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>EscalationReasonPicklistScreen</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Emergency Escalation</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>HelpFromHQText</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 20px;&quot;&gt;Please only use this escalation reason if you do not understand the member, cannot send an initial reply, or do not know what to do.&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>EscalationReasonPicklistScreen</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Need help from HQ</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>InsideSalesChoiceText</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 20px;&quot;&gt;Use this reason for messages/ calls from people that want to apply to live at Common, tour our homes, etc. Often people who have called the wrong number.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 20px;&quot;&gt;DO NOT use for people who have yet to apply / tour. More info here: &lt;/span&gt;&lt;span style=&quot;color: rgb(0, 149, 255); font-size: 20px;&quot;&gt;https://docs.google.com/document/d/1kJZndTfw_nvwkDwcg2EGaelPvmMsUE9USwb9z_Jvydc/edit#&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>EscalationReasonPicklistScreen</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Inside Sales Queue (not a member yet)</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>LeasingQueueChoiceText</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 20px;&quot;&gt;Use this reason for messages/ calls from prospective members who want to know about the status of their application / have submitted a deposit + application.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 20px;&quot;&gt;DO NOT use for people who haven’t toured or applied yet. More Info here: &lt;/span&gt;&lt;span style=&quot;color: rgb(65, 166, 255); font-size: 20px;&quot;&gt;https://docs.google.com/document/d/1kJZndTfw_nvwkDwcg2EGaelPvmMsUE9USwb9z_Jvydc/edit#&lt;span class=&quot;ql-cursor&quot;&gt;﻿&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>EscalationReasonPicklistScreen</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Leasing Admin Queue (in application)</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
