            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>or</conditionLogic>
                <conditions>
                    <leftValueReference>EscalationReasonPicklistScreen</leftValueReference>
                    <operator>EqualTo</operator>
