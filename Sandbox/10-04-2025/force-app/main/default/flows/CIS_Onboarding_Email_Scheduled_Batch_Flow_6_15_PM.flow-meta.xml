<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionCalls>
        <description>This chatter notification is for posting to our admin chatter group so that we can resolve the issues with a record that didn&apos;t send an on-boarding notification. At the time of writing the only scenario where this would post is if a record is found in the GET nodes but it didn&apos;t have an onboarding email defined.</description>
        <name>Internal_Chatter_Notification_No_Template_ID_Found</name>
        <label>Internal Chatter Notification - No Template ID Found</label>
        <locationX>578</locationX>
        <locationY>468</locationY>
        <actionName>chatterPost</actionName>
        <actionType>chatterPost</actionType>
        <flowTransactionModel>CurrentTransaction</flowTransactionModel>
        <inputParameters>
            <name>text</name>
            <value>
                <stringValue>{!$Record.Name} met the criteria for getting a onboarding email but was not send one due to it&apos;s suite not having an onboarding email defined. Please update the suite&apos;s onboarding email template and then this record will be send the onboarding email at the next scheduled flow time.</stringValue>
            </value>
        </inputParameters>
        <inputParameters>
            <name>subjectNameOrId</name>
            <value>
                <stringValue>0F91i0000008ffsCAA</stringValue>
            </value>
        </inputParameters>
        <inputParameters>
            <name>type</name>
            <value>
                <stringValue>Group</stringValue>
            </value>
        </inputParameters>
        <nameSegment>chatterPost</nameSegment>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </actionCalls>
    <apiVersion>49.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Any_Record_Found</name>
        <label>Any Record Found</label>
        <locationX>611</locationX>
        <locationY>252</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>True</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>If_On_Boarding_Template_Defined</targetReference>
            </connector>
            <label>True</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_On_Boarding_Template_Defined</name>
        <label>If On-Boarding Template Defined</label>
        <locationX>380</locationX>
        <locationY>360</locationY>
        <defaultConnector>
            <targetReference>Internal_Chatter_Notification_No_Template_ID_Found</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>True1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Home_On_boarding_Email_SendGrid_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>is_start_date_between_14_and_60_days</targetReference>
            </connector>
            <label>True</label>
        </rules>
    </decisions>
    <decisions>
        <name>is_start_date_between_14_and_60_days</name>
        <label>is start date between 14 and 60 days?</label>
        <locationX>182</locationX>
        <locationY>468</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Start_Date_of_Occupancy__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>fourteenDaysFromNow</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Start_Date_of_Occupancy__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>sixtyDaysFromNow</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CIS_On_boarding_Email_Sub_Flow</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>NOTE: PLEASE USE V4 IF THIS REQUIRES ROLLBACK</description>
    <environments>Default</environments>
    <formulas>
        <name>fourteenDaysFromNow</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 14</expression>
    </formulas>
    <formulas>
        <name>sixtyDaysFromNow</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 60</expression>
    </formulas>
