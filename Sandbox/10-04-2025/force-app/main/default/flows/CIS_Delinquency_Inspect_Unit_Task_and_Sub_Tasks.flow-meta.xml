1, {!$Flow.CurrentDate} +2, 
2, {!$Flow.CurrentDate} +2, 
3, {!$Flow.CurrentDate}  +2, 
4, {!$Flow.CurrentDate} +4, 
5, {!$Flow.CurrentDate} +4, 
6, {!$Flow.CurrentDate} +3,
null
)</expression>
