    <formulas>
        <name>max_budget_formula</name>
        <dataType>Currency</dataType>
        <expression>IF(
AND(NOT(ISBLANK({!$Record.Calendly__CustomResponse3__c})), 
ISNUMBER(SUBSTITUTE({!$Record.Calendly__CustomResponse3__c}, &quot;$&quot;, &quot;&quot;))),
VALUE(SUBSTITUTE({!$Record.Calendly__CustomResponse3__c}, &quot;$&quot;, &quot;&quot;)),
NULL
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>min_budget_formula</name>
        <dataType>Currency</dataType>
        <expression>IF(
AND(NOT(ISBLANK({!$Record.Calendly__CustomResponse2__c})), 
ISNUMBER(SUBSTITUTE({!$Record.Calendly__CustomResponse2__c}, &quot;$&quot;, &quot;&quot;))),
VALUE(SUBSTITUTE({!$Record.Calendly__CustomResponse2__c}, &quot;$&quot;, &quot;&quot;)),
NULL
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>move_in_date</name>
        <dataType>Date</dataType>
        <expression>IF(
ISBLANK({!$Record.Calendly__CustomResponse4__c}), NULL, 
DATE(
    VALUE(RIGHT({!$Record.Calendly__CustomResponse4__c}, 4)),
    VALUE(LEFT({!$Record.Calendly__CustomResponse4__c}, 2)),
    VALUE(MID({!$Record.Calendly__CustomResponse4__c}, 4, 2))
)
)</expression>
    </formulas>
    <formulas>
        <name>TodayPlus30</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate}+30</expression>
    </formulas>
