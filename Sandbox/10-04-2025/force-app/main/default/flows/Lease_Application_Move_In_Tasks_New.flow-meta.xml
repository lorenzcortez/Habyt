    <description>New version based on home fields. Updated to include Habyt record type IDs.</description>
    <environments>Default</environments>
    <formulas>
        <name>OnsiteMoveInTaskOwner</name>
        <dataType>String</dataType>
        <expression>{!OpportunityRecord.OwnerId}</expression>
    </formulas>
    <formulas>
        <name>OnsiteMoveInTaskOwnerFormula</name>
        <dataType>String</dataType>
        <expression>IF ( ISBLANK({!OnsiteMoveInTaskOwner}) , &quot;0056A000000xcIn&quot;, {!OnsiteMoveInTaskOwner})</expression>
    </formulas>
    <formulas>
        <name>PSMoveInTaskOwner</name>
        <dataType>String</dataType>
        <expression>IF( AND(TEXT({!OpportunityRecord.Home_Committed__r.Property_Services_Move_in_Task_Owner__c}) == &apos;Porter&apos;, {!OpportunityRecord.Home_Committed__r.Porter__r.IsActive} == True),
{!OpportunityRecord.Home_Committed__r.Porter__c},
IF( AND(TEXT({!OpportunityRecord.Home_Committed__r.Property_Services_Move_in_Task_Owner__c}) == &apos;Community Manager&apos;, {!OpportunityRecord.Home_Committed__r.Community_Manager__r.IsActive} == True),
{!OpportunityRecord.Home_Committed__r.Community_Manager__c},

IF( AND( TEXT({!OpportunityRecord.Home_Committed__r.Property_Services_Move_in_Task_Owner__c}) == &apos;Leasing Specialist&apos;, {!OpportunityRecord.Home_Committed__r.Leasing_Specialist__r.IsActive} == True),
{!OpportunityRecord.Home_Committed__r.Leasing_Specialist__c},
IF(
{!OpportunityRecord.Home_Committed__r.Maintenance_Tech__r.IsActive}  == True,
{!OpportunityRecord.Home_Committed__r.Maintenance_Tech__c},
null
))))</expression>
    </formulas>
    <formulas>
        <name>PSMoveInTaskOwnerFormula</name>
        <dataType>String</dataType>
        <expression>/*IF ( ISBLANK( PSMoveInTaskOwner ) , &quot;0056A000000xcIn&quot;, PSMoveInTaskOwner )*/
IF ( ISBLANK( {!PSMoveInTaskOwner} ) , &quot;0051i000004i3ikAAA&quot;, {!PSMoveInTaskOwner} )</expression>
    </formulas>
    <formulas>
        <name>StartDateMinus4</name>
        <dataType>Date</dataType>
        <expression>{!OpportunityRecord.Start_Date_of_Occupancy__c} - 4</expression>
    </formulas>
    <formulas>
        <name>StartDateMinusOne</name>
        <dataType>Date</dataType>
        <expression>{!OpportunityRecord.Start_Date_of_Occupancy__c} - 1</expression>
    </formulas>
    <formulas>
        <name>TodayPlusSeven</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate}+7</expression>
    </formulas>
