<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Add_to_Platform_Event_List</name>
        <label>Add to Platform Event List</label>
        <locationX>264</locationX>
        <locationY>758</locationY>
        <assignmentItems>
            <assignToReference>SEList</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>SE</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_over_Suite_Opps</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Set_SG_Platform_Event_Record</name>
        <label>Set SG Platform Event Record</label>
        <locationX>264</locationX>
        <locationY>638</locationY>
        <assignmentItems>
            <assignToReference>SE.From_Email__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>brandFromEmail</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.From_Name__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>OppFromBrand</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.SendGrid_Template_Id__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>SGEmailTemplateID</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.To_Email__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>SingleSuiteOpp.Email__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.Opportunity_ID__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>SingleSuiteOpp.Id</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.First_Name__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>SingleSuiteOpp.Account_First_Name__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.End_Date_of_Occupancy__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>EndDateOfOccupancy</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.Room_Name__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Get_Opportunity_Fields.Room_Selected__r.Name</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.Inspection_Date__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>get_Task.ActivityDate</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>SE.Suite_Name__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>SingleSuiteOpp.ApartmentRef__r.Name</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Add_to_Platform_Event_List</targetReference>
        </connector>
    </assignments>
    <formulas>
        <name>brandFromEmail</name>
        <dataType>String</dataType>
        <expression>CASE( Text( {!Get_Opportunity_Fields.Brand__c}  ), 
&quot;noah&quot;,&quot;residents@noahapartments.com&quot;,  
&quot;kin&quot;,&quot;members@milyliving.com&quot;, 
&apos;members@common.com&apos;)</expression>
    </formulas>
    <formulas>
        <name>EndDateOfOccupancy</name>
        <dataType>Date</dataType>
        <expression>IF(ISBLANK({!Get_Opportunity_Fields.End_Date_of_Occupancy__c}), 
{!Get_Opportunity_Fields.Previous_Occupancy_End_Date__c}, 
{!Get_Opportunity_Fields.End_Date_of_Occupancy__c}
)</expression>
    </formulas>
    <formulas>
        <name>OppFromBrand</name>
        <dataType>String</dataType>
        <expression>CASE( Text({!Get_Opportunity_Fields.Brand__c} ), 
&quot;noah&quot;,&quot;Noah Member Services&quot;,  
&quot;kin&quot;,&quot;Mily Member Services&quot;, 
&apos;Common Member Services&apos;)</expression>
    </formulas>
    <interviewLabel>Inspection Email Notification {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CSS - Inspection Email Notification</label>
    <loops>
        <name>Loop_over_Suite_Opps</name>
        <label>Loop over Suite Opps</label>
        <locationX>176</locationX>
        <locationY>518</locationY>
        <assignNextValueToReference>SingleSuiteOpp</assignNextValueToReference>
        <collectionReference>Get_SuiteMate_Opportunities</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Set_SG_Platform_Event_Record</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Create_Platform_Event_Records</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
        <name>Create_Platform_Event_Records</name>
        <label>Create Platform Event Records</label>
        <locationX>176</locationX>
        <locationY>974</locationY>
        <inputReference>SEList</inputReference>
    </recordCreates>
    <recordLookups>
        <description>This will get the fields needed for the emails from the opportunity.</description>
        <name>Get_Opportunity_Fields</name>
        <label>Get Opportunity Fields</label>
        <locationX>176</locationX>
        <locationY>158</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>get_Task</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>OpportunityId</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Opportunity</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <name>Get_SuiteMate_Opportunities</name>
        <label>Get SuiteMate Opportunities</label>
        <locationX>176</locationX>
        <locationY>398</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Loop_over_Suite_Opps</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Email__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Occupancy_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Active</stringValue>
            </value>
        </filters>
        <filters>
            <field>ApartmentRef__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>Get_Opportunity_Fields.ApartmentRef__c</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>Opportunity</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <name>get_Task</name>
        <label>get Task</label>
        <locationX>176</locationX>
        <locationY>278</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Get_SuiteMate_Opportunities</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Subject</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Pre-Move-Out Inspection</stringValue>
            </value>
        </filters>
        <filters>
            <field>WhatId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>OpportunityId</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Task</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Opportunity_Fields</targetReference>
        </connector>
    </start>
