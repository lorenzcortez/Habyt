    <formulas>
        <name>FormulaSkipVR</name>
        <dataType>Boolean</dataType>
        <expression>AND(
ISPICKVAL({!var_Contract.Status}, &quot;Terminated&quot;),
OR(
ISNULL({!var_Contract.Contract_End_Date__c}),
ISNULL({!var_Contract.Move_out_date__c}),
ISNULL({!var_Contract.Notice_Date__c}),
ISBLANK(TEXT({!var_Contract.Reason_of_Termination__c}))),
{!var_Contract.RecordTypeId} =&quot;0121i000000YFBw&quot;
)</expression>
    </formulas>
