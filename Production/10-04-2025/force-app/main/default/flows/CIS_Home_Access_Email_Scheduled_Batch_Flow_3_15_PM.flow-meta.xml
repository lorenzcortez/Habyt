<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Opportunity_Type_Evaluation</name>
        <label>Opportunity Type Evaluation</label>
        <locationX>176</locationX>
        <locationY>252</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Transfer_Upon_Renewal</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Transfer_Upon_Renewal__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Yes</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CIS_Home_Access_Email_Sub_Flow</targetReference>
            </connector>
            <label>Transfer Upon Renewal</label>
        </rules>
        <rules>
            <name>Initial_Back_fill_Or_Transfer</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Common_Backfill</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Common_Initial</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Common_Transfer</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CIS_Home_Access_Email_Sub_Flow</targetReference>
            </connector>
            <label>Initial, Back-fill Or Transfer</label>
        </rules>
    </decisions>
    <description>This flow is scheduled flow process records in batches and passing to sub flow to create platform event record for each record in we get from batch. (Move-in/Move-out</description>
    <environments>Default</environments>
    <interviewLabel>CIS - Home Access Email Scheduled Batch Flow 3:15 AM {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CIS - Home Access Email Scheduled Batch Flow 3:15 PM</label>
    <processMetadataValues>
