        </connector>
        <filterLogic>1 AND (2 OR 3 OR 4 OR 5) AND 6 AND 7 AND 8 AND 9 AND 10</filterLogic>
        <filters>
            <field>Home_Committed__c</field>
            <operator>IsNull</operator>
