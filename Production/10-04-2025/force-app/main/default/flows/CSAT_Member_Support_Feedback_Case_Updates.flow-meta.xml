    <decisions>
        <name>Case_Owner_Evaluation</name>
        <label>Case Owner Evaluation</label>
        <locationX>976</locationX>
        <locationY>270</locationY>
        <defaultConnector>
            <targetReference>Reopen_The_Case</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Assigned to Queue/Case Blank</defaultConnectorLabel>
        <rules>
            <name>Assigned_to_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Feedback_Record.Case__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Feedback_Record.Case__r.OwnerId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>005</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Feedback_Owner</targetReference>
            </connector>
            <label>Assigned to User</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_Feedback_Type_is_Member_Support</name>
        <label>If Feedback Type is Member Support</label>
        <locationX>642</locationX>
        <locationY>269</locationY>
        <defaultConnector>
            <targetReference>Update_Case</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Good Score</defaultConnectorLabel>
        <rules>
            <name>Bad_Score</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Feedback_Record.Score__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>2.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_Owner_Evaluation</targetReference>
            </connector>
            <label>Bad Score</label>
        </rules>
    </decisions>
