    <formulas>
        <name>AccountFirstName</name>
        <dataType>String</dataType>
        <expression>IF( NOT (ISBLANK(  {!Get_Opportunity.Account.Preferred_First_Name__c} )  ) , {!Get_Opportunity.Account.Preferred_First_Name__c} , {!Get_Opportunity.Account.FirstName})</expression>
    </formulas>
    <formulas>
        <name>Evaultcode_formula</name>
        <dataType>String</dataType>
        <expression>IF(ISNULL({!EVaultObject.eVault_Code__c}), &quot;&quot;, {!EVaultObject.eVault_Code__c})</expression>
    </formulas>
    <formulas>
        <name>EvaultLocation_Formula</name>
        <dataType>String</dataType>
        <expression>IF(ISNULL({!EVaultObject.Location__c}), &quot;&quot;, {!EVaultObject.Location__c})</expression>
    </formulas>
    <formulas>
        <name>EVaultName_Formula</name>
        <dataType>String</dataType>
        <expression>IF(ISNULL({!EVaultObject.Product_name__c}), &quot;&quot;, MID({!EVaultObject.Product_name__c}, 5, LEN({!EVaultObject.Product_name__c})))</expression>
    </formulas>
    <formulas>
        <name>FromEmail</name>
        <dataType>String</dataType>
        <expression>if({!Get_Opportunity.BrandRef__r.Name} = &apos;Common&apos;, &apos;leasing@common.com&apos;, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;Noah&apos;, &apos;residents@noahapartments.com&apos;, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;Mily&apos;, &apos;members@milyliving.com&apos;, null)))</expression>
    </formulas>
    <formulas>
        <name>FromName</name>
        <dataType>String</dataType>
        <expression>if({!Get_Opportunity.BrandRef__r.Name} = &apos;Common&apos;, &apos;Common Leasing Administration Team&apos;, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;Noah&apos;, &apos;Noah Leasing Team&apos;, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;Mily&apos;, &apos;Mily Leasing Administration Team&apos;, null)))</expression>
    </formulas>
    <formulas>
        <name>HomeMoveInHours_Formula</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE( {!Get_Opportunity.Home_Committed__r.Move_In_Hours__c} , &apos;&quot;&apos; , &apos;\\&quot;&apos;)</expression>
    </formulas>
    <formulas>
        <name>ReplyEmail</name>
        <dataType>String</dataType>
        <expression>if({!Get_Opportunity.BrandRef__r.Name} = &apos;common&apos;, &apos;leasing@common.com&apos;, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;noah&apos;, &apos;residents@noahapartments.com&apos;, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;mily&apos;, &apos;members@milyliving.com&apos;, null)))</expression>
    </formulas>
    <formulas>
        <name>SendGridID</name>
        <dataType>String</dataType>
        <expression>if({!Get_Opportunity.BrandRef__r.Name} = &apos;Common&apos;, {!Get_Opportunity.Home_Committed__r.Access_System__r.Home_Access_SendGrid_ID__c}, 
if({!Get_Opportunity.BrandRef__r.Name} = &apos;Noah&apos;, {!Get_Opportunity.Home_Committed__r.Access_System__r.Noah_Home_Access_SendGrid_ID__c}, 
