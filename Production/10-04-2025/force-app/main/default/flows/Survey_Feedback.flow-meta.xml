        <name>BedroomnotclearnCommonandKin</name>
        <dataType>String</dataType>
        <expression>IF( 
TEXT({!Feedback.Brand__c}) == &quot;kin&quot;, 
&quot;My unit was not clean.&quot;, 
&quot;My bedroom was not clean.&quot;
)</expression>
    </formulas>
